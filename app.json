{
  "expo": {
    "name": "OVO",
    "slug": "ovo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0286FF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ovoLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/ovoLogo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://ovo.dev/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
